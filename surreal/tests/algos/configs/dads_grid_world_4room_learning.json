{
  "type": "DADS",
  "episode_buffer_capacity": 200,

  "policy_network": [
    {
      "name": "dense",
      "units": 128,
      "activation": "tanh"
    },
    {
      "name": "dense",
      "units": 128,
      "activation": "tanh"
    }
  ],
  "q_network": [
    {
      "name": "dense",
      "units": 256,
      "activation": "tanh"
    },
    {
      "name": "dense",
      "units": 256,
      "activation": "tanh"
    }
  ],
  "num_q_experts": 4,
