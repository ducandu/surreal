        "units": 128,
        "activation": "tanh"
      }
    ],

    "default_optimizer": {
      "type": "adam",
      "learning_rate": 0.0001
    },

    "q_optimizer": {
      "type": "adam",
      "learning_rate": 0.005
    },

    "time_unit": "time_step",
    "update_after": "when-memory-ready",
    "update_frequency": 1,
    "num_steps_per_update": 1,
    "sync_frequency": 1,
    "sync_tau": 0.005
  }
