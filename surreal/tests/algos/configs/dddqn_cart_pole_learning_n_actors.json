{
  "type": "DDDQN",
  "gamma": 0.99,

  "n_step": 5,

  "memory_capacity": 5000,
  "memory_batch_size": 256,

  "epsilon": {
    "type": "linear-decay",
    "from": 1.0,
    "to": 0.05,
    "end_time_percentage": 0.9
  },

  "q_network": [
    {
      "name": "dense",
      "units": 20,
      "activation": "tanh"
    }
  ],

  "dueling_a_network": [
    {
      "name": "dense",
      "units": 5,
      "activation": "relu"
    }
  ],

  "dueling_v_network": [
    {
      "name": "dense",
      "units": 5,
      "activation": "relu"
    }
  ],

  "update_frequency": 4,
  "update_after": "when-memory-ready",
  "sync_frequency": 32,

  "optimizer": {
    "type": "adam",
    "learning_rate": [0.01, 0.0001],
    "clip_norm": 40
  }
}
