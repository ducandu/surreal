{
  "gamma": 0.99,

  "n_step": 2,

  "memory_capacity": 512,
  "memory_batch_size": 64,

  "epsilon": {
    "type": "linear_decay",
    "from": 1.0,
    "to": 0.0,
    "end_percentage": 0.95
  },

  "q_network": [
    {
      "name": "dense",
      "units": 5,
      "activation": "linear"
    }
  ],

  "dueling_v_network": [
    {
      "name": "dense",
      "units": 5
    }
  ],

  "dueling_a_network": [
    {
      "name": "dense",
      "units": 5
    }
  ],

  "update_frequency": 4,
  "update_after": "when-memory-ready",
  "sync_frequency": 16,

  "optimizer": {
    "type": "adam",
    "learning_rate": {
      "type": "linear_decay",
      "from": 0.003,
      "to": 0.0,
      "end_percentage": 0.95
    }
  }
}
