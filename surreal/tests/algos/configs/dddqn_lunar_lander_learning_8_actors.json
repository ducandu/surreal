{
  "gamma": 0.99,

  "memory_capacity": 100000,
  "memory_batch_size": 256,

  "epsilon": {
    "type": "linear-decay",
    "from": 1.0,
    "to": 0.0,
    "end_time_percentage": 0.9
  },

  "q_network": [
    {
      "name": "dense",
      "units": 512,
      "activation": "relu"
    },
    {
      "name": "dense",
      "units": 512,
      "activation": "relu"
    },
    {
      "name": "dense",
      "units": 512,
      "activation": "relu"
    }
  ],

  "dueling_a_network": [
    {
      "name": "dense",
      "units": 32,
      "activation": "relu"
    }
  ],

  "dueling_v_network": [
    {
      "name": "dense",
      "units": 32,
      "activation": "relu"
    }
  ],

  "update_frequency": 4,
  "steps_before_update": "when-memory-ready",
  "sync_frequency": 64,

  "optimizer": {
    "type": "adam",
    "learning_rate": 0.0001,
    "clip_norm": 40
  }
}
