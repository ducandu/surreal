{
  "gamma": 0.99,

  "n_step": 6,
  "n_step_only": false,

  "memory_capacity": 20000,
  "memory_batch_size": 256,

  "epsilon": {
    "type": "linear-decay",
    "from": 1.0,
    "to": 0.05,
    "end_time_percentage": 0.9
  },

  "q_network": [
    {
      "name": "dense",
      "units": 128,
      "activation": "tanh"
    },
    {
      "name": "dense",
      "units": 128,
      "activation": "tanh"
    }
  ],

  "dueling_a_network": [
    {
      "name": "dense",
      "units": 64,
      "activation": "relu"
    }
  ],

  "dueling_v_network": [
    {
      "name": "dense",
      "units": 64,
      "activation": "relu"
    }
  ],

  "update_frequency": 1,
  "steps_before_update": "when-memory-ready",
  "sync_frequency": 64,

  "optimizer": {
    "type": "adam",
    "learning_rate": [0.01, 0.00001],
    "clip_norm": 40
  }
}
