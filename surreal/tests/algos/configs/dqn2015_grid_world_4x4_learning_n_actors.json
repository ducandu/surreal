{
  "gamma": 1.0,

  "memory_capacity": 256,
  "memory_batch_size": 64,

  "epsilon": {
    "type": "linear-decay",
    "from": 1.0,
    "to": 0.1,
    "end_time_percentage": 0.8
  },

  "q_network": [
    {
      "name": "dense",
      "units": 20,
      "activation": "relu"
    },
    {
      "name": "dense",
      "units": 20,
      "activation": "relu"
    }
  ],

  "update_frequency": 8,
  "update_after": "when-memory-full",
  "sync_frequency": 64,

  "optimizer": {
    "type": "adam",
    "learning_rate": [0.001, 0.00001]
  }
}
