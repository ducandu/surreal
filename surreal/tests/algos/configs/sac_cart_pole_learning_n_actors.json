{
  "type": "SAC",
  "gamma": 0.99,
  "memory_capacity": 5000,
  "memory_batch_size": 256,
  "initial_alpha": 0.5,

  "gumbel_softmax_temperature": 0.5,
  "n_step": 3,

  "num_q_networks": 2,

  "q_network": [
    {
      "name": "dense",
      "units": 128,
      "activation": "tanh"
    },
    {
      "name": "dense",
      "units": 128,
      "activation": "tanh"
    }
  ],

  "default_optimizer": {
    "type": "adam",
    "learning_rate": [0.001, 0.00005],
    "clip_norm": 40
  },

  "q_optimizer": {
    "type": "adam",
    "learning_rate": [0.001, 0.0001],
    "clip_norm": 40
  },

  "time_unit": "time_step",
  "update_after": "when-memory-ready",
  "update_frequency": 1,
  "num_steps_per_update": 1,
  "sync_frequency": 1,
  "sync_tau": 0.005
}
