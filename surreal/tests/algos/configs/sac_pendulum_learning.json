{
  "type": "SAC",
  "gamma": 0.99,


  "memory_capacity": 10000,
  "memory_batch_size": 256,

  "initial_alpha": 1.0,

  "num_q_networks": 2,
  "q_network": [
    {
      "name": "dense",
      "units": 256,
      "activation": "tanh"
    },
    {
      "name": "dense",
